filter f_mining0 { match("wolk-mining0" value("PROGRAM") value("MSG") value("MSGHDR")); };
destination d_mining0 {
        file("/disk1/log0/$YEAR/$MONTH/$DAY/$HOUR$MIN.mining"
        owner( root ) group( root ) perm ( 0777 ) dir_perm( 0777 ) create_dirs ( yes )
        template ( "$MESSAGE\n" ) );
};
log { source(s_tcp_net); filter(f_mining0); destination(d_mining0); };

