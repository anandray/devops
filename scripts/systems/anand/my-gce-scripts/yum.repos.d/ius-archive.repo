[ius-archive]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Archive
#baseurl=https://dl.iuscommunity.org/pub/ius/archive/Redhat/6/$basearch
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo=ius-el6-archive&arch=$basearch&protocol=http
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY

